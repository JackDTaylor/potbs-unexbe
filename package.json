{
  "name": "jdt-potbs",
  "version": "1.0.0",
  "description": "My custom express application boilerplate",
  "repository": "https://github.com/JackDTaylor/jdt-express-boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "touch ./tmp/compile.log && (webpack -w --config backend.webpack.config.js > ./tmp/compile.log & webpack -w --config frontend.webpack.config.js > ./tmp/compile.log & tail -f ./tmp/compile.log)",
    "build-frontend": "webpack --config frontend.webpack.config.js",
    "build-backend": "webpack --config backend.webpack.config.js",
    "build-vendor": "webpack --config vendor.webpack.config.js",
    "watch-frontend": "webpack -w --config frontend.webpack.config.js",
    "watch-backend": "webpack -w --config backend.webpack.config.js",
    "watch-vendor": "webpack -w --config vendor.webpack.config.js",
    "generate-icon-font": "node ./utils/font-generator.js",
    "log": "tail -f --lines=30 ./tmp/console.log -q 2>&1 | perl -ne 'if (/tail: \\.\\/tmp\\/console.log: file truncated/) {system 'clear'} else {print}'",
    "kill": "if ( kill $( ps -eo pid,args | grep 'Passenger NodeApp' | grep -v 'grep Passenger NodeApp' | awk '{print $1}') > /dev/null 2>&1 ); then echo 'Process killed'; else echo 'Not Running'; fi;"
  },
  "author": "Jack D. Taylor",
  "license": "ISC",
  "dependencies": {
    "@devexpress/dx-grid-core": "latest",
    "@devexpress/dx-react-core": "^1.1.1",
    "@devexpress/dx-react-grid": "^1.1.1",
    "@devexpress/dx-react-grid-material-ui": "^1.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-bluebird": "^1.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chokidar": "latest",
    "clean-webpack-plugin": "^0.1.18",
    "client-sessions": "^0.8.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "fast-csv": "^2.4.1",
    "html-webpack-plugin": "^3.0.5",
    "icon-font-generator": "^1.1.1",
    "jquery": "^3.3.1",
    "json-formatter-js": "^2.2.0",
    "knex": "^0.14.6",
    "material-ui": "^1.0.0-beta.44",
    "material-ui-icons": "^1.0.0-beta.36",
    "material-ui-time-picker": "^0.1.6",
    "md5": "^2.2.1",
    "moment": "^2.22.0",
    "mysql2": "^1.5.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-flatpickr": "^3.6.4",
    "react-popper": "^0.10.4",
    "react-select": "^1.2.1",
    "react-swipeable-views": "^0.12.13",
    "react-tap-event-plugin": "^3.0.2",
    "webpack": "^3.11.0",
